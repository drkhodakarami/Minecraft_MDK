{
  "schemaVersion": 1,
  "id": "modmdk",
  "version": "${version}",
  "name": "The Mod Development Kit",
  "description": "This mod is a template for developing fabric mods",
  "authors": [
    "Jiraiyah"
  ],
  "contact": {
    "homepage": "https://fabricmc.net/",
    "sources": "https://github.com/FabricMC/fabric-example-mod"
  },
  "license": "MIT",
  "icon": "assets/modmdk/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "jiraiyah.modmdk.ModMain"
    ],
    "fabric-datagen": [
      "jiraiyah.modmdk.ATMDataGenerator"
    ],
    "client": [
      "jiraiyah.modmdk.ModClient"
    ],
    "rei_client": [
      "jiraiyah.modmdk.compat.ModREIClientPlugin"
    ]
  },
  "mixins": [
    "modmdk.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.14.24",
    "minecraft": "~1.20.2",
    "java": ">=17",
    "fabric-api": "*"
  },
  "suggests": {
    "another-mod": "*"
  }
}